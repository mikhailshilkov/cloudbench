{
    "version": "2.0",
    "extensions": {
      "http": {        
        "maxConcurrentRequests": 1,
        "maxOutstandingRequests": 4
      }
    },
    "aggregator": {
        "batchSize": 1000,
        "flushTimeout": "00:00:30"
    },
    "logging": {
        "applicationInsights": {
            "sampling": {
              "isEnabled": true,
              "maxTelemetryItemsPerSecond" : 5
            }
        }
    }
}